{
    "version": 2,
    "builds": [{
        "src": "time_table/time_table/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.8" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "time_table/static"
        }
    }],

    "routes": [
        {
            "src": "/static/(.*)*",
            "dest": "time_table/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "time_table/time_table/wsgi.py"
        }
        
    ]
}